# For debuging:
# 1) snapcraft define desktop-qt5
# 2) export QT_DEBUG_PLUGINS=1
# 3) export SNAPCRAFT_DEBUG=true

# Rebuild, making sure official qt is used
cd ..
PATH=/usr/bin:$PATH
LD_LIBRARY_PATH=
qmake engauge.pro CONFIG+=log4cpp_null
make clean
make -j4

cd snap
rm -rf parts prime snap stage
snapcraft clean engauge
snapcraft
sudo snap install engauge_10.6_amd64.snap --devmode --dangerous
